<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_7.0.xsd">

    <grid name="axelor.script.model.grid" width="large" title="Models" editable="true"
          model="com.axelor.meta.db.MetaModel">
        <field name="name" readonly="true"/>
        <field name="fullName"/>
        <field name="generate"/>
    </grid>

    <form title="Fields" name="axelor.script.field.form" model="com.axelor.meta.db.MetaField">
        <panel-dashlet action="open-axelor-script-field-grid-view"/>
    </form>

    <grid name="axelor.script.field.grid" title="Fields" model="com.axelor.meta.db.MetaField" editable="true">
        <field name="metaModel"/>
        <field name="name"/>
        <field name="generate"/>
    </grid>

    <action-view name="open-axelor-script-field-grid-view" title="Fields">
        <view type="grid" name="axelor.script.field.grid"/>
        <domain>metaModel = :metaModel</domain>
    </action-view>

</object-views>